<form method="post" action="{{ path('app_animal_breed_delete', {'id': animal_breed.id}) }}" 
      onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette race ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ animal_breed.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
